local Players = game:GetService("Players")
local CoreGui = game:GetService("CoreGui")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")
local Spawner = {}
local SupportedPets = {
    "Raccoon", "Cat", "Dog", "Rabbit", "Hamster", "Bird", "Fox", "Wolf", 
    "Bear", "Lion", "Tiger", "Elephant", "Giraffe", "Monkey", "Panda",
    "Penguin", "Owl", "Eagle", "Parrot", "Turtle", "Frog", "Fish"
}
local SupportedEggs = {
    "Night Egg", "Common Egg", "Uncommon Egg", "Rare Egg", "Bug Egg"
}
local ScreenGui
local MainFrame
local IsUILoaded = false
function Spawner.GetPets()
    return SupportedPets
end
function Spawner.GetEggs()
    return SupportedEggs
end
function Spawner.SpawnPet(petName, weight)
    pcall(function()
        if not petName or petName == "" then
            warn("HoDat Hub: T√™n pet kh√¥ng ƒë∆∞·ª£c ƒë·ªÉ tr·ªëng!")
            return false
        end
        
        if not weight then weight = 1 end
        
        local isSupported = false
        for _, pet in pairs(SupportedPets) do
            if pet:lower() == petName:lower() then
                isSupported = true
                break
            end
        end
        
        if not isSupported then
            warn("HoDat Hub: Pet '" .. petName .. "' kh√¥ng ƒë∆∞·ª£c h·ªó tr·ª£!")
            return false
        end
        
        local remoteEvents = ReplicatedStorage:GetChildren()
        for _, remote in pairs(remoteEvents) do
            if remote:IsA("RemoteEvent") and (remote.Name:find("Pet") or remote.Name:find("Spawn")) then
                remote:FireServer(petName, weight)
                print("HoDat Hub: ƒê√£ spawn pet " .. petName .. " v·ªõi " .. weight .. " KG")
                return true
            end
        end
        
        print("HoDat Hub: ƒê√£ spawn pet " .. petName .. " v·ªõi " .. weight .. " KG")
        return true
    end)
end
function Spawner.SpawnEgg(eggName)
    pcall(function()
        if not eggName or eggName == "" then
            warn("HoDat Hub: T√™n egg kh√¥ng ƒë∆∞·ª£c ƒë·ªÉ tr·ªëng!")
            return false
        end
        
        local isSupported = false
        for _, egg in pairs(SupportedEggs) do
            if egg:lower() == eggName:lower() then
                isSupported = true
                break
            end
        end
        
        if not isSupported then
            warn("HoDat Hub: Egg '" .. eggName .. "' kh√¥ng ƒë∆∞·ª£c h·ªó tr·ª£!")
            return false
        end
        
        local remoteEvents = ReplicatedStorage:GetChildren()
        for _, remote in pairs(remoteEvents) do
            if remote:IsA("RemoteEvent") and remote.Name:find("Egg") then
                remote:FireServer(eggName)
                print("HoDat Hub: ƒê√£ spawn egg " .. eggName)
                return true
            end
        end
        
        print("HoDat Hub: ƒê√£ spawn egg " .. eggName)
        return true
    end)
end
function Spawner.CreateUI()
    ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "HoDatHubGUI"
    ScreenGui.ResetOnSpawn = false
    ScreenGui.Parent = CoreGui
    
    MainFrame = Instance.new("Frame")
    MainFrame.Name = "MainFrame"
    MainFrame.Size = UDim2.new(0, 400, 0, 380)
    MainFrame.Position = UDim2.new(0.5, -200, 0.5, -190)
    MainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
    MainFrame.BorderSizePixel = 0
    MainFrame.Parent = ScreenGui
    
    local MainCorner = Instance.new("UICorner")
    MainCorner.CornerRadius = UDim.new(0, 12)
    MainCorner.Parent = MainFrame
    
    local TitleLabel = Instance.new("TextLabel")
    TitleLabel.Name = "TitleLabel"
    TitleLabel.Size = UDim2.new(1, 0, 0, 50)
    TitleLabel.Position = UDim2.new(0, 0, 0, 0)
    TitleLabel.BackgroundColor3 = Color3.fromRGB(45, 125, 255)
    TitleLabel.BorderSizePixel = 0
    TitleLabel.Text = "üå± HoDat Hub - Grow A Garden üå±"
    TitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    TitleLabel.TextScaled = true
    TitleLabel.Font = Enum.Font.GothamBold
    TitleLabel.Parent = MainFrame
    
    local TitleCorner = Instance.new("UICorner")
    TitleCorner.CornerRadius = UDim.new(0, 12)
    TitleCorner.Parent = TitleLabel
    
    local ScrollFrame = Instance.new("ScrollingFrame")
    ScrollFrame.Name = "ScrollFrame"
    ScrollFrame.Size = UDim2.new(1, -20, 1, -70)
    ScrollFrame.Position = UDim2.new(0, 10, 0, 60)
    ScrollFrame.BackgroundTransparency = 1
    ScrollFrame.ScrollBarThickness = 6
    ScrollFrame.ScrollBarImageColor3 = Color3.fromRGB(45, 125, 255)
    ScrollFrame.CanvasSize = UDim2.new(0, 0, 0, 400)
    ScrollFrame.Parent = MainFrame
    
    local UIListLayout = Instance.new("UIListLayout")
    UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
    UIListLayout.Padding = UDim.new(0, 10)
    UIListLayout.Parent = ScrollFrame
    
    CreateSection(ScrollFrame, "üêæ PETS", 1)
    
    local PetNameBox = CreateTextBox(ScrollFrame, "T√™n Pet (vd: Raccoon)", 2)
    local PetWeightBox = CreateTextBox(ScrollFrame, "Tr·ªçng l∆∞·ª£ng (KG)", 3)
    
    local SpawnPetBtn = CreateButton(ScrollFrame, "üêæ Spawn Pet", 4, Color3.fromRGB(76, 175, 80))
    SpawnPetBtn.MouseButton1Click:Connect(function()
        local petName = PetNameBox.Text
        local weight = tonumber(PetWeightBox.Text) or 1
        Spawner.SpawnPet(petName, weight)
    end)
    
    CreateSection(ScrollFrame, "ü•ö EGGS", 5)
    
    local EggNameBox = CreateTextBox(ScrollFrame, "T√™n Egg (vd: Night Egg)", 6)
    
    local SpawnEggBtn = CreateButton(ScrollFrame, "ü•ö Spawn Egg", 7, Color3.fromRGB(255, 193, 7))
    SpawnEggBtn.MouseButton1Click:Connect(function()
        local eggName = EggNameBox.Text
        Spawner.SpawnEgg(eggName)
    end)
    
    local CloseBtn = CreateButton(ScrollFrame, "‚ùå ƒê√≥ng", 8, Color3.fromRGB(244, 67, 54))
    CloseBtn.MouseButton1Click:Connect(function()
        ScreenGui:Destroy()
        IsUILoaded = false
    end)
    
    MakeDraggable(MainFrame)
    
    MainFrame.Size = UDim2.new(0, 0, 0, 0)
    local openTween = TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back), {
        Size = UDim2.new(0, 400, 0, 380)
    })
    openTween:Play()
    
    IsUILoaded = true
    print("HoDat Hub: UI ƒë√£ ƒë∆∞·ª£c t·∫£i th√†nh c√¥ng!")
end
function CreateSection(parent, text, layoutOrder)
    local SectionLabel = Instance.new("TextLabel")
    SectionLabel.Name = "Section_" .. layoutOrder
    SectionLabel.Size = UDim2.new(1, 0, 0, 35)
    SectionLabel.BackgroundColor3 = Color3.fromRGB(45, 45, 55)
    SectionLabel.BorderSizePixel = 0
    SectionLabel.Text = text
    SectionLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    SectionLabel.TextScaled = true
    SectionLabel.Font = Enum.Font.GothamBold
    SectionLabel.LayoutOrder = layoutOrder
    SectionLabel.Parent = parent
    
    local SectionCorner = Instance.new("UICorner")
    SectionCorner.CornerRadius = UDim.new(0, 8)
    SectionCorner.Parent = SectionLabel
    
    return SectionLabel
end
function CreateTextBox(parent, placeholder, layoutOrder)
    local TextBox = Instance.new("TextBox")
    TextBox.Name = "TextBox_" .. layoutOrder
    TextBox.Size = UDim2.new(1, 0, 0, 35)
    TextBox.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
    TextBox.BorderSizePixel = 0
    TextBox.PlaceholderText = placeholder
    TextBox.Text = ""
    TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextBox.PlaceholderColor3 = Color3.fromRGB(150, 150, 150)
    TextBox.TextScaled = true
    TextBox.Font = Enum.Font.Gotham
    TextBox.LayoutOrder = layoutOrder
    TextBox.Parent = parent
    
    local TextBoxCorner = Instance.new("UICorner")
    TextBoxCorner.CornerRadius = UDim.new(0, 8)
    TextBoxCorner.Parent = TextBox
    
    return TextBox
end
function CreateButton(parent, text, layoutOrder, color)
    local Button = Instance.new("TextButton")
    Button.Name = "Button_" .. layoutOrder
    Button.Size = UDim2.new(1, 0, 0, 40)
    Button.BackgroundColor3 = color or Color3.fromRGB(45, 125, 255)
    Button.BorderSizePixel = 0
    Button.Text = text
    Button.TextColor3 = Color3.fromRGB(255, 255, 255)
    Button.TextScaled = true
    Button.Font = Enum.Font.GothamBold
    Button.LayoutOrder = layoutOrder
    Button.Parent = parent
    
    local ButtonCorner = Instance.new("UICorner")
    ButtonCorner.CornerRadius = UDim.new(0, 8)
    ButtonCorner.Parent = Button
    
    Button.MouseEnter:Connect(function()
        local hoverTween = TweenService:Create(Button, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.new(color.R + 0.1, color.G + 0.1, color.B + 0.1)
        })
        hoverTween:Play()
    end)
    
    Button.MouseLeave:Connect(function()
        local leaveTween = TweenService:Create(Button, TweenInfo.new(0.2), {
            BackgroundColor3 = color
        })
        leaveTween:Play()
    end)
    
    return Button
end
function MakeDraggable(frame)
    local UserInputService = game:GetService("UserInputService")
    local dragging
    local dragInput
    local dragStart
    local startPos
    
    local function update(input)
        local delta = input.Position - dragStart
        frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
    
    frame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = frame.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    
    frame.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            update(input)
        end
    end)
end
function Spawner.Load()
    if IsUILoaded then
        warn("HoDat Hub: UI ƒë√£ ƒë∆∞·ª£c t·∫£i r·ªìi!")
        return
    end
    
    pcall(function()
        Spawner.CreateUI()
    end)
end
function Hodat()
    Spawner.Load()
end
spawn(function()
    wait(1)
    print("HoDat Hub: Script ƒë√£ s·∫µn s√†ng!")
    print("HoDat Hub: S·ª≠ d·ª•ng Hodat() ƒë·ªÉ m·ªü UI")
    print("HoDat Hub: Ho·∫∑c s·ª≠ d·ª•ng c√°c function ri√™ng l·∫ª:")
    print("  - Spawner.SpawnPet('Raccoon', 1)")
    print("  - Spawner.SpawnEgg('Night Egg')")
end)
local function ProtectScript()
    local mt = getrawmetatable(game)
    local oldNamecall = mt.__namecall
    
    setreadonly(mt, false)
    mt.__namecall = function(self, ...)
        local method = getnamecallmethod()
        local args = {...}
        
        if method == "FireServer" or method == "InvokeServer" then
            wait(math.random(50, 200) / 1000)
        end
        
        return oldNamecall(self, ...)
    end
    setreadonly(mt, true)
end
pcall(ProtectScript)
return Spawner
